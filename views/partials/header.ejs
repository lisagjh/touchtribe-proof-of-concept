    <section class="header-product">

        <h1>
            <%= newProduct.name %>
        </h1>

        <% if (newProduct.image) { %>
            <picture>
                <source srcset="<%= newProduct.image.url %>?fm=avif&w=300" type="image/avif"
                    media="(max-width: 500px)">
                <source srcset="<%= newProduct.image.url %>?fm=avif&w=400" type="image/avif"
                    media="(min-width: 501px)">
                <source srcset="<%= newProduct.image.url %>?fm=webp&w=300" type="image/webp"
                    media="(max-width: 500px)">
                <source srcset="<%= newProduct.image.url %>?fm=webp&w=400" type="image/webp"
                    media="(min-width: 501px)">
                <img src="<%= newProduct.image.url %>" alt="<%= newProduct.title %>">
            </picture>
            <% } else { %>
                <img src="/assets/sweater.avif" alt="sweater" width="300">
                <% } %>


                    


                    <p>
                        <%= newProduct.description %>
                    </p>


    </section>

</header>